{
  "name": "redux-fetch-resource",
  "version": "0.0.0-semantic-versioning",
  "description": "A Complete solution for making Requests in redux applications",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuid": "rimraf lib",
    "build": "npm run-all --parallel build:*",
    "build:main": "babel src --out lib",
    "build:umd": "webpack --output-filename redux-fetch-resource.min.js -p",
    "test": "_mocha --compilers js:babel-register __TESTS__",
    "test:cover": "nyc npm t",
    "test:watch": "npm t -- -w",
    "coverage:check": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "stickycube",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "mocha": "3.0.2",
    "nyc": "8.3.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "semistandard": "9.0.0",
    "webpack": "1.13.2"
  },
  "peerDependencies": {
    "react": ">=0.x.x"
  },
  "dependencies": {
    "warning": "3.0.0"
  },
  "semistandard": {
    "parser": "babel-eslint"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StickyCube/redux-fetch-resource.git"
  }
}
